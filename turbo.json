{
    "tasks": {
      "build": {
        "outputs": [".next/**", "dist/**"],
        "cache": true,
        "dependsOn": ["^build"]
      },
      "dev": {
        "cache": false,
        "persistent": true
      },
      "lint": {
        "outputs": [],
        "cache": true
